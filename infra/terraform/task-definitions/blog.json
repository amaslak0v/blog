[
    {
      "name": "blog",
      "image": "025772230981.dkr.ecr.eu-central-1.amazonaws.com/blog:3c5e86e-2019-11-20",
      "cpu": 10,
      "memory": 512,
      "essential": true,
      "executionRoleArn": "arn:aws:iam::025772230981:role/ecsTaskExecutionRole",
